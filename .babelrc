{
  "env": {
    "development": {
      "sourceMaps": "inline",
      "plugins": [["source-map-support"]]
    }
  },
  "presets": [["babel-preset-gatsby-package"]],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "rootPathSuffix": "./src/",
        "rootPathPrefix": "~/"
      }
    ],
    [
      "@babel/plugin-proposal-private-methods",
      {
        "loose": false
      }
    ],
    [
      "import-globals",
      {
        "dd": {
          "moduleName": "dumper.js",
          "exportName": "dd"
        },
        "dump": {
          "moduleName": "dumper.js",
          "exportName": "dump"
        },
        "clipboardy": {
          "moduleName": "clipboardy",
          "exportName": "default"
        }
      }
    ]
  ]
}
